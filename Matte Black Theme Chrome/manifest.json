{
    "version": "1.7",
    "description": "A modern Matte Black theme",
    "manifest_version": 2,
    "name": "Matte Black Theme",
    "short_name": "Matte Black",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "theme": {
        "colors": {
            "frame": [0, 0, 0],
            "toolbar": [35, 35, 35],
            "tab_background_text": [170, 170, 170],
            "tab_text": [255, 255, 255],
            "background_tab_inactive": [18, 18, 18],
            "frame_inactive": [18, 18, 18],
            "ntp_background": [18, 18, 18],
            "ntp_header": [18, 18, 18],
            "ntp_link": [255, 255, 255],
            "ntp_link_underline": [35, 35, 35],
            "ntp_section": [255, 255, 255],
            "ntp_section_link": [136, 136, 136],
            "ntp_section_link_underline": [136, 136, 136],
            "ntp_section_text": [136, 136, 136],
            "ntp_text": [136, 136, 136],
            "bookmark_text": [170, 170, 170]
        },

        "tints": {
            "background_tab": [-1, 0.5, 0],
            "background_tab_incognito": [-1, 0.5, 0]
        },
        "properties": {
            "ntp_background_alignment": "top center",
            "ntp_background_repeat": "no-repeat",
            "ntp_logo_alternate": 0
        }
    }
}
